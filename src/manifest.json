{
  "name": "TimeReporter",
  "version": "0.0.0.1",
  "manifest_version": 2,
  "description": "Populates work time in MSVacation WTT.",
  "icons": {
    "16": "icons/timerecorder16x16.png",
    "32": "icons/timerecorder32x32.png",
    "48": "icons/timerecorder48x48.png",
    "128": "icons/timerecorder128x128.png"
  },
  "background": {
    "scripts": ["lib/moment.min.js", "background.js"],
    "persistent": false
  },
  "browser_action": {

  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
        "lib/jquery.min.js",
        "content.js"]
    }
  ]
}
